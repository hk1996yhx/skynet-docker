include "config.path"
--必须配置
--启用多少个工作线程
thread = 8
--启动的第一个服务
bootstrap = "snlua bootstrap"
--主服务入口
start = "main"
--不使用主从节点模式
harbor = 0
--后台模式（必要时开启）
--preload  = "preload.lua"
--daemon ="../skynet.pid"
-- debug console
debug_console_port = 4040
--日志模块
logger      = "9_logger"
logservice  = "snlua"
logpath     = "zlog"
--debug|info|warn|error
log_level   = "debug"
logtag      = "node1"
--节点
node ="node1"
